    agent any

    stages {
       stage('Hello') {
            steps {
                echo 'Hello, World!'
            }
        }
        stage('Git') {
            steps {
                echo 'we need to push code into git'
            }
        }
        stage('doc') {
            steps {
                echo 'docker will create docker images'
            }
        }
    }
}

